			<!--show_connector_types_menu>false</show_connector_types_menu-->
			<show_overlay_types_menu>false</show_overlay_types_menu>
		</connection_menu>
		<callback>
			<on_load_task_properties>DBTableTaskPropertyObj.onLoadTaskProperties</on_load_task_properties>
			<on_submit_task_properties>DBTableTaskPropertyObj.onSubmitTaskProperties</on_submit_task_properties>
			<on_cancel_task_properties></on_cancel_task_properties>
			
			<on_load_connection_properties>DBTableTaskPropertyObj.onLoadConnectionProperties</on_load_connection_properties>
			<on_submit_connection_properties>DBTableTaskPropertyObj.onSubmitConnectionProperties</on_submit_connection_properties>

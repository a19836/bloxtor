		<css>../../common/css/global.css</css>
		<css>../common/css/global.css</css>
		<css>css/WorkFlowTask.css</css>
		<js>../../common/js/global.js</js>
		<js>../common/js/global.js</js>
		<js>js/WorkFlowTask.js</js>
	</files>
	<settings>

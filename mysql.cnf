[mysqld]
sql-mode="ONLY_FULL_GROUP_BY,STRICT_TRANS_TABLES,ERROR_FOR_DIVISION_BY_ZERO,NO_ENGINE_SUBSTITUTION"
max_allowed_packet=250M
wait_timeout=28800
mysql_native_password=ON

[mysqld_safe]
max_allowed_packet=100M

[client]
max_allowed_packet=100M

[mysql]
max_allowed_packet=100M

[mysqldump]
max_allowed_packet=100M